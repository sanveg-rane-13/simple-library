<!-- Default sign-in page rendered by devise -->

<div class="row">
  <div class="col-xs-12 col-sm-6 col-sm-offset-4">
    <%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name), :class => "form-inline") do |f| %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    <%= f.password_field :password %>

    <br>
    <%= f.submit "Log In" %>
    <% end %>
  </div>
</div>

<%= render "devise/shared/links" %>